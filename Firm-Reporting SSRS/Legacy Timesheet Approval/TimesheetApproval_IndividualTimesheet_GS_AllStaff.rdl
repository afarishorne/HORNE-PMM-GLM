  <DataSets>
    <DataSet Name="Activity_Codes_By_Day">
      <Query>
        <DataSourceName>PracticeEngine</DataSourceName>
        <CommandText>SELECT top 10
	ts.StaffId
,	ts.StaffName
,	StartDate = CAST('4/1/2020' as DATETIME)
,	EndDate = CAST('4/15/2020'  as DATETIME)
,	SUM(ts.ChargeableHours) AS BillHours
,	ClientGroup = 33582
FROM
	dbo.client_vw_Timesheets ts
JOIN
	dbo.tblEngagement E ON E.ContIndex=ts.ContIndex
WHERE 
	ts.EntryDate BETWEEN '4/1/2020' AND '4/15/2020'
AND E.ClientHold = (SELECT ContIndex FROM dbo.tblContacts WHERE ContName='xxxxxNCORR-SINGLE FAMILY HOUSING' )
AND ts.StaffId IN (SELECT StaffId FROM bpmdata.HorneDataWarehouse.OLAP.StaffDimension)
GROUP BY 
	ts.StaffId,ts.StaffName 
HAVING SUM(ts.ChargeableHours) &gt; 0
ORDER BY
	ts.StaffId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="StaffId">
          <DataField>StaffId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StaffName">
          <DataField>StaffName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="BillHours">
          <DataField>BillHours</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ClientGroup">
          <DataField>ClientGroup</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ClientGroup">
      <Query>
        <DataSourceName>PracticeEngine</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED; 

SELECT DISTINCT C.ContName,C.ContIndex
FROM
	tblPortfolio_Selection P 
JOIN
	tblEngagement E ON P.ContIndex = E.ContIndex
JOIN
	tblContacts C ON P.ClientHold = C.ContIndex
WHERE 
	e.clientorganisation = 3 
ORDER BY C.ContName</CommandText>
      </Query>
      <Fields>
        <Field Name="ContName">
          <DataField>ContName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ContIndex">
          <DataField>ContIndex</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Staff">
      <Query>
        <DataSourceName>PracticeEngine</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ClientGroup">
            <Value>=Parameters!ClientGroup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*



SELECT DISTINCT
	staff.FirstName + ' ' + staff.LastName AS FullName
,	StaffId
FROM
	HorneDataWarehouse.OLAP.StaffDimension staff
WHERE
	Active=1
OR (Active = 0 AND staff.DateLeft &gt;= GETDATE()-365)
ORDER BY
	FullName
*/
SELECT DISTINCT ts.StaffId, ts.StaffName
FROM     client_vw_Timesheets AS ts INNER JOIN
                  tblEngagement AS E ON E.ContIndex = ts.ContIndex
WHERE  (ts.EntryDate BETWEEN @StartDate AND @EndDate) AND (E.ClientHold IN
                      (SELECT ContIndex
                       FROM      tblContacts
                       WHERE   (ContIndex = @ClientGroup))) AND (ts.StaffId IN
                      (SELECT ts.StaffId
                       FROM      BPMData.HorneDataWarehouse.OLAP.StaffDimension AS StaffDimension_1))
ORDER BY ts.StaffName</CommandText>
      </Query>
      <Fields>
        <Field Name="StaffId">
          <DataField>StaffId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StaffName">
          <DataField>StaffName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Selections">
      <Query>
        <DataSourceName>dsFirmAdmin</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Staff">
            <Value>=Parameters!Staff.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT
	staff.FirstName + ' ' + staff.LastName AS FullName
,	StaffId
FROM
	HorneDataWarehouse.OLAP.StaffDimension staff
WHERE
	StaffId IN (@Staff)
ORDER BY
	FullName</CommandText>
      </Query>
      <Fields>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StaffId">
          <DataField>StaffId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
