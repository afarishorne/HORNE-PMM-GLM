  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT FocusArea AS [Focus Area], Sum(NewHires) AS [New Hires], Sum(Terminations) AS [Terminations] FROM 
(

SELECT sd.FocusArea,
       COUNT(sd.StaffBKey) AS NewHires,
       0 AS Terminations
FROM DM.vw_StaffDetail AS sd
WHERE CAST(sd.SeniorityDate AS DATE)
BETWEEN @StartDate AND @EndDate
GROUP BY FocusArea
UNION ALL 
SELECT sd.FocusArea,
       0 AS NewHires,
       COUNT(sd.StaffBKey) AS Teriminations
FROM DM.vw_StaffDetail AS sd
WHERE CAST(sd.TerminationDate AS DATE)
BETWEEN @StartDate AND @EndDate
GROUP BY FocusArea
)a
GROUP BY FocusArea
ORDER BY FocusArea</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Focus_Area">
          <DataField>Focus Area</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="New_Hires">
          <DataField>New Hires</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Terminations">
          <DataField>Terminations</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DateFilter">
      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <CommandText>SELECT 1 AS Value, 'Last 7 Days' AS Description, 1 AS Indexer
UNION
SELECT 2 AS Value, 'Last 30 days' AS Description, 2 AS Indexer
UNION
SELECT 3 AS Value, 'Last Week' AS Description, 3 AS Indexer
UNION
SELECT 4 AS Value, 'Last Month' AS Description, 4 AS Indexer
ORDER BY Indexer, Value</CommandText>
      </Query>
      <Fields>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Indexer">
          <DataField>Indexer</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="StartDate">
      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFilter">
            <Value>=Parameters!DateFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- 1 = Last 7 days
IF @DateFilter = 1
BEGIN
    --start of last 7 days
    SELECT CAST(GETDATE() - 7 AS DATE) AS StartDate
END
-- 2 = Last 30 days
ELSE IF @DateFilter = 2
BEGIN
    --start of last 30 days
    SELECT CAST(GETDATE() - 30 AS DATE) AS StartDate
END
-- 3 = Last Week
ELSE IF @DateFilter = 3
BEGIN
    --start of last week
    SELECT DATEADD(wk, DATEDIFF(wk, 6, GETDATE()), 0) AS StartDate
END
-- 4 = Last Month
ELSE IF @DateFilter = 4
BEGIN
    --First day of last month
    SELECT DATEADD(m, -1, DATEADD(mm, DATEDIFF(m, 0, GETDATE()), 0)) AS StartDate
END</CommandText>
      </Query>
      <Fields>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="EndDate">
      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFilter">
            <Value>=Parameters!DateFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- 1 = Last 7 days
IF @DateFilter = 1
BEGIN
    --end of last 7 days
    SELECT CAST(GETDATE() - 1 AS DATE) AS EndDate
END
-- 2 = Last 30 days
ELSE IF @DateFilter = 2
BEGIN
    --end of last 30 days
    SELECT CAST(GETDATE() - 1 AS DATE) AS EndDate
END
-- 3 = Last Week
ELSE IF @DateFilter = 3
BEGIN
    --end of last week
    SELECT DATEADD(wk, DATEDIFF(wk, 6, GETDATE()), 6) AS EndDate
END
-- 4 = Last Month
ELSE IF @DateFilter = 4
BEGIN
    --Last day of last month
    SELECT DATEADD(d, -1, DATEADD(mm, DATEDIFF(m, 0, GETDATE()), 0)) AS EndDate
END</CommandText>
      </Query>
      <Fields>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
