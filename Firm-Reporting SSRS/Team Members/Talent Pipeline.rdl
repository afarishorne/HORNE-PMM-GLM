    <DataSet Name="seniorityDate">
      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Filter">
            <Value>=Parameters!Filter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
       [Seniority Date] = sd.SeniorityDate
 
FROM
      DM.vw_StaffDetail AS sd
--INNER JOIN  DM.vw_StaffDetail AS vsd ON sd.TerminationDate = vsd.TerminationDate
WHERE   
  CASE	
		WHEN @Filter &gt; 0 AND
			(CAST (sd.SeniorityDate AS DATE) &lt;= CAST(DATEADD(DAY, @Filter, getdate()) AS DATE)
				OR CAST (sd.LoadDate AS DATE) &lt;= CAST(DATEADD(DAY, @Filter, getdate()) AS DATE)
			) AND DATEDIFF(DAY, GETDATE(), sd.SeniorityDate) &gt;=0 THEN 1
		WHEN @Filter &lt;= 0 AND
			(CAST (sd.SeniorityDate AS DATE) &gt;= CAST(DATEADD(DAY, @Filter, getdate()) AS DATE)
				OR CAST (sd.LoadDate AS DATE) &gt;= CAST(DATEADD(DAY, @Filter, getdate()) AS DATE)
			) AND DATEDIFF(DAY, GETDATE(), sd.SeniorityDate) &lt;=0 THEN 1

		ELSE 0 
		END = 1

ORDER BY sd.SeniorityDate DESC	</CommandText>
      </Query>
      <Fields>
        <Field Name="Seniority_Date">
          <DataField>Seniority Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="lastMonthTermination">
      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <CommandText>----Termination count for the Last Month

SELECT 
	  COUNT(sd.TerminationDate)
FROM
      DM.vw_StaffDetail AS sd
WHERE
       (SD.TerminationDate &gt;= DATEADD(mm, DATEDIFF(mm, 0, GETDATE()) - 1, 0)   -----first day of the previous month
	   AND sd.TerminationDate &lt;= DATEADD(DAY, -(DAY(GETDATE())), GETDATE()))  ---last day of the previous month:
ORDER BY COUNT(SD.TerminationDate) DESC

</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="lastMonthSeniority">
      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <CommandText>--- SeniorityDate count for the Last Month
SELECT 
	  COUNT(sd.SeniorityDate)
FROM
      DM.vw_StaffDetail AS sd
WHERE
      (SD.SeniorityDate &gt;= DATEADD(mm, DATEDIFF(mm, 0, GETDATE()) - 1, 0)   -----first day of the previous month
	   AND sd.SeniorityDate &lt;= DATEADD(DAY, -(DAY(GETDATE())), GETDATE()))  ---last day of the previous month:
	   
ORDER BY COUNT(sd.SeniorityDate) DESC 
</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
