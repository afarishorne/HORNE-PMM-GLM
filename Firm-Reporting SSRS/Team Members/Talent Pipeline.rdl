    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@focusArea">
            <Value>=Parameters!focusArea.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@costCenter">
            <Value>=Parameters!costCenter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@staffClassification">
            <Value>=Parameters!staffClassification.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Filter">
            <Value>=Parameters!Filter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>---master data sets 
SELECT 
        [Focus Area] = sd.FocusArea
       ,[Seniority Date] = sd.SeniorityDate
        ,[Termination Date] = sd.TerminationDate
        ,[HRMS ID] = sd.StaffBKey
        ,sd.LastName
        , sd.CostCenter
        ,sd.StaffClassification
        ,CONVERT(VARCHAR(10),DATEDIFF(DAY,sd.SeniorityDate, GETDATE())) as [Days Since Hired]  
        ,CASE WHEN  CONVERT(VARCHAR(10),DATEDIFF(DAY,sd.TerminationDate, GETDATE())) IN ('-357289') THEN 'N/A' --'-357301', '-357300',
		 ELSE CONVERT(VARCHAR(10),DATEDIFF(DAY,sd.TerminationDate, GETDATE()))
		 END AS [Days Since Termination]
 
FROM
      DM.vw_StaffDetail AS sd
--INNER JOIN  DM.vw_StaffDetail AS vsd ON sd.TerminationDate = vsd.TerminationDate
WHERE   
       sd.FocusArea IN (@focusArea)
       AND sd.CostCenter IN (@costCenter)
       AND sd.StaffClassification IN (@staffClassification)
       AND 
       CASE	
		WHEN @Filter &gt; 0 AND
			(CAST (sd.SeniorityDate AS DATE) &lt;= CAST(DATEADD(DAY, @Filter, getdate()) AS DATE)
				OR CAST (sd.LoadDate AS DATE) &lt;= CAST(DATEADD(DAY, @Filter, getdate()) AS DATE)
			) AND DATEDIFF(DAY, GETDATE(), sd.SeniorityDate) &gt;=0 THEN 1
		WHEN @Filter &lt;= 0 AND
			(CAST (sd.SeniorityDate AS DATE) &gt;= CAST(DATEADD(DAY, @Filter, getdate()) AS DATE)
				OR CAST (sd.LoadDate AS DATE) &gt;= CAST(DATEADD(DAY, @Filter, getdate()) AS DATE)
			) AND DATEDIFF(DAY, GETDATE(), sd.SeniorityDate) &lt;=0 THEN 1

		ELSE 0 
		END = 1

ORDER BY sd.SeniorityDate DESC		</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Focus_Area">
          <DataField>Focus Area</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
