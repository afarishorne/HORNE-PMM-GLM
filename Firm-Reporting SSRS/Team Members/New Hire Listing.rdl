      <Query>
        <DataSourceName>HORNE</DataSourceName>
        <CommandText>SELECT								     

 [First Name]  = S.PreferredName

,[Last Name]  = S.LastName

,[Email] = S.EmailAddress

FROM
    
 [DM].[StaffDetail] AS S

WHERE s.SeniorityDate = CONVERT(VARCHAR(10), DATEADD(dd,-1,GETDATE()), 101)</CommandText>
