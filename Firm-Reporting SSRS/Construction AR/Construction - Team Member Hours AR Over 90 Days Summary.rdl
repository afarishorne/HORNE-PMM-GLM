  <DataSources>
    <DataSource Name="Reporting_Engine_Horne">
      <DataSourceReference>Reporting_Engine_Horne</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>56bc953d-8a07-4584-8306-069990877da8</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Reporting_Engine_Horne</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ClientID">
            <Value>=Parameters!ClientID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH TOTAL AS (
SELECT DISTINCT
	eng.ClientCode AS ClientID
	,[Job_Code] AS EventId
	,AnalysisCode AS workcodeid
	,[ServiceCode] AS engagementid
	,EntryDate AS 'Date' 
	,DurationUnits AS 'Hours'
	,staff.StaffName
	,eng.ClientName
	,c.ContName ClientGroup
	,staff.StaffOffice
	,det.ChargeAmount
	,wip.WIPAmount
	,'Description' = det.Description
	,Task_Subject
	,j.Job_Name

FROM [Engine_Horne].[dbo].[tblTSO_Details] det
INNER JOIN [Engine_Horne].[dbo].[tblTSO_Header] head 
	ON det.headerindex = head.headerindex
INNER JOIN [Engine_Horne].[dbo].[tblStaff] staff 
	ON staff.StaffIndex = head.StaffIndex
INNER JOIN [Engine_Horne].[dbo].[tblEngagement] eng 
	ON eng.ContIndex = det.clientindex
LEFT JOIN [Engine_Horne].[dbo].[tblJob_Header] J 
	ON det.ServPeriod = J.Job_Idx
LEFT JOIN [Engine_Horne].[dbo].[tblJob_Task] task 
	ON task.taskindex = det.taskindex
LEFT JOIN [Engine_Horne].[dbo].[tblTimeChargeCode] AS TC 
	ON det.AnalysisCode = TC.ChargeCode
LEFT JOIN [Engine_Horne].[dbo].[tblContacts] C 
	ON eng.ClientHold = C.ContIndex
LEFT JOIN [Engine_Horne].[dbo].[tblStaff] ClientPartner 
	ON ClientPartner.StaffIndex = eng.ClientPartner
LEFT JOIN [Engine_Horne].[dbo].[tblTranWIP] wip 
	ON det.detailindex = wip.wipref

WHERE staff.StaffDepartment ='CON'

	and det.EntryDate Between @StartDate and @EndDate
	AND (eng.ClientCode IN (@ClientID) OR @ClientID IS NULL)

UNION ALL

--EXPENSES
SELECT DISTINCT 
	eng.ClientCode AS ClientID 
	,[Job_Code] AS EventId
	,[DisbCode] AS workcodeid
	,[ServiceCode] AS engagementid
	,det.expDate AS 'Date'
	,NULL AS 'Hours'
	,staff.StaffName
	,eng.ClientName
	,c.ContName ClientGroup
	,staff.StaffOffice
	,ChargeAmount
	,wip.WIPAmount	
	,'Description' = det.Description
	,NULL AS Task_Subject
	,j.Job_Name

FROM [Engine_Horne].[dbo].[tblExpenseDetail] det
INNER JOIN [Engine_Horne].[dbo].[tblExpenseHeader] head 
	ON det.expindex = head.expindex
INNER JOIN [Engine_Horne].[dbo].[tblStaff] staff 
	ON staff.StaffIndex = head.ExpStaff
INNER JOIN [Engine_Horne].[dbo].[tblEngagement] eng 
	ON eng.ContIndex = det.clientindex
LEFT JOIN [Engine_Horne].[dbo].[tblJob_Header] J 
	ON det.ServPeriod = J.Job_Idx
LEFT JOIN [Engine_Horne].[dbo].[tblTimeChargeCode] AS TC 
	ON det.DisbCode = TC.ChargeCode
LEFT JOIN [Engine_Horne].[dbo].[tblContacts] C 
	ON eng.ClientHold = C.ContIndex
LEFT JOIN [Engine_Horne].[dbo].[tblStaff] ClientPartner 
	ON ClientPartner.StaffIndex = eng.ClientPartner
LEFT JOIN [Engine_Horne].[dbo].[tblTranWIP] wip 
	ON det.detailindex = wip.wipref
	
WHERE staff.StaffDepartment ='CON'

	and det.expDate Between @StartDate and @EndDate
	AND (eng.ClientCode IN (@ClientID) OR @ClientID IS NULL)
		
) 

SELECT DISTINCT
[StaffName]
,StaffOffice Office
,[ClientID]
,[ClientName] OrganizationName
,ClientGroup
,Sum([Hours]) Hours
,Sum(WIPAmount) as WIPAmount
,ar.AROutstanding as ClientAROver90Days

FROM TOTAL t
join
(
SELECT [ClientCode]
      ,[Client]
      ,Sum([AROutstanding]) as AROutstanding
  FROM [DashboardMaster].[dbo].[vw_FocusAreaARAgingDetails]
  where FocusArea='Construction' and ARAging ='[90+]'
  and AROutstanding&gt;0
  group by
  [ClientCode]
      ,[Client]

)ar on t.ClientID = ar.ClientCode
group by
[StaffName]
,StaffOffice
,[ClientID]
,[ClientName]
,ClientGroup
,ar.AROutstanding</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
